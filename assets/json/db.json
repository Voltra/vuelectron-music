{
	"server": "vuelectron-music",
	"version": 1,
	"schema": {
		"musics": {
			"key": {"keyPath": "id", "autoIncrement": true},
			"indexes": {
				"path": {"unique": true},
				"title": {},
				"duration": {},
				"artist": {},
				"album": {},
				"year": {},
				"genre": {},
				"date_added": {},
				"plays": {}
			}
		}
	}
}