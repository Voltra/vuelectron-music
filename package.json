{
  "name": "vuelectron-music",
  "version": "1.0.0",
  "description": "A desktop music player made using Vue and Electron",
  "main": "index.js",
  "pkg": {
    "assets": [
      "node_modules/ncp/*",
      "node_modules/promisify-node/*"
    ]
  },
  "build": {
    "appId": "com.voltra.vuelectron-music",
    "productName": "Vuelectron Music",
    "directories": {
      "output": "release",
      "app": "."
    },
    "buildDependenciesFromSource": true,
    "buildVersion": "0.0.1"
  },
  "scripts": {
    "setup": "npm i && npm up",
    "start": "electron .",
    "build-g": "npm run build-e && npm run build",
    "prodBuild-g": "npm run prodBuild-e && npm run prodBuild",
    "build-e": "cross-env NODE_ENV=dev webpack --config electron.webpack.config.js --progress --colors",
    "prodBuild-e": "cross-env NODE_ENV=prod webpack --config electron.webpack.config.js --progress --colors",
    "watch-e": "cross-env NODE_ENV=prod webpack --config electron.webpack.config.js --progress --colors",
    "build": "cross-env NODE_ENV=dev webpack --config app.webpack.config.js --progress --colors",
    "prodBuild": "cross-env NODE_ENV=prod webpack --config app.webpack.config.js --progress --colors",
    "watch": "cross-env NODE_ENV=dev webpack --config app.webpack.config.js --progress --colors --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Voltra/vuelectron-music.git"
  },
  "author": "Ludwig GUERIN <ludwig.guerin.98@gmx.fr>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Voltra/vuelectron-music/issues"
  },
  "homepage": "https://github.com/Voltra/vuelectron-music#readme",
  "devDependencies": {
    "babel-core": "^6.26.2",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-do-expressions": "^6.22.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.19",
    "compass-mixins": "^0.12.10",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "electron": "^1.8.6",
    "electron-builder": "^20.10.0",
    "file-loader": "^1.1.11",
    "img-loader": "^2.0.1",
    "mixin-loader": "^2.0.3",
    "ncp": "^2.0.0",
    "node-sass": "^4.9.0",
    "pkg": "^4.3.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "vue-loader": "^14.2.2",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-manifest-plugin": "^2.0.1"
  },
  "dependencies": {
    "@voltra/json": "^1.2.0",
    "dashify": "^1.0.0",
    "db.js": "^0.15.0",
    "express": "^4.16.3",
    "fetch-ponyfill": "^6.0.2",
    "liquidjs": "^3.1.2",
    "lodash.defaultsdeep": "^4.6.0",
    "lodash.merge": "^4.6.1",
    "music-metadata": "^0.10.3",
    "perfect-scrollbar": "^1.3.0",
    "promisify-node": "^0.4.0",
    "spinner-lord": "^0.2.6",
    "store": "^2.0.12",
    "vue": "^2.5.16",
    "vue-async-computed": "^3.3.1",
    "vue-router": "^3.0.1",
    "vue-tables-2": "^1.4.31",
    "vuex": "^3.0.1"
  }
}
